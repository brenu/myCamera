{
  "expo": {
    "name": "myCamera",
    "slug": "mycamera",
    "privacy": "public",
    "platforms": ["ios", "android", "web"],
    "version": "0.4.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#444444"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#444444"
    },
    "androidNavigationBar": {
      "barStyle": "light-content",
      "backgroundColor": "#444444"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.brenu.camera",
      "versionCode": 4
    }
  }
}
